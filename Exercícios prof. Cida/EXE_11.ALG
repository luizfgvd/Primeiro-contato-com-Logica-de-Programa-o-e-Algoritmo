algoritmo "exe_11"
var
   GrauCelsius, Fahr : Real
inicio
      Escreval ("Digite a temperatura em graus Celsius: ")
      Leia (GrauCelsius)
      Fahr <- (9 * GrauCelsius + 160) / 5
      Escreval ( "A temperatura ", GrauCelsius , " °C ", "convertida em Graus Fahreinheit é: ", Fahr)
fimalgoritmo