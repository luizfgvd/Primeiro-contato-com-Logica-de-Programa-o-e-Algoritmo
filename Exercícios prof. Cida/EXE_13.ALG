algoritmo "Exe_13"
var
   A, B, C : Inteiro
   S : Real
inicio
      Escreval ("Digite o primeiro valor inteiro: ")
      Leia (A)
      Escreval ("Digite o segundo valor inteiro: ")
      Leia (B)
      Escreval ("Digite o terceiro valor inteiro: ")
      Leia (C)
      S <- (A^2)+(B^2)+(C^2)
      Escreval ("A soma entre o quadrado dos valores inteiros é : ", S)
fimalgoritmo